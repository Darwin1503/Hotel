    <EditText
        android:id="@+id/txtidcliente"
        android:layout_width="217dp"
        android:layout_height="47dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:inputType="textPersonName"
